<Window x:Class="ControlChart.PrintPreviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        Title="Print Preview" Height="1122" Width="793">
    <Grid>
        <Grid  x:Name="PrintArea" Grid.Row="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <StackPanel Grid.Row="0" Orientation="Horizontal" Margin="1,1,1,1">
                <Label x:Name="labelDocName" Content="Xbar-Rs-R Control Chart" HorizontalAlignment="Left" Margin="10,0,0,0" />
                <Button x:Name="PrintList" Content="プリンタ一覧" Margin="5" Click="PrinterList_Click"/>
                <Button x:Name="PrintButton" Content="印刷" Margin="5" Click="PrintButton_Click"/>
                <Button x:Name="ExitButton" Content="終了" Margin="5" Click="ExitButton_Click"/>
                <Label x:Name="labelDate" Content="YYYY 年 MM 月 DD 日" HorizontalAlignment="Right" Margin="350,0,10,0" FontSize="10" />
            </StackPanel>
            <!-- ヘッダ -->
            <Grid x:Name="Header" Grid.Row="1" Margin="1,1,1,1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>

                <!-- Header Row -->
                <Border Grid.Row="0" Grid.Column="0" BorderBrush="Black" Background="LightGray" BorderThickness="1,1,0.5,0.5">
                    <TextBlock Text="項目コード" Margin="2"/>
                </Border>
                <Border Grid.Row="0" Grid.Column="1" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="textK_CODE" Margin="2"/>
                </Border>
                <Border Grid.Row="0" Grid.Column="2" BorderBrush="Black" Background="LightGray" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock Text="検査項目名" Margin="2"/>
                </Border>
                <Border Grid.Row="0" Grid.Column="3" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="textK_NAME" Margin="2"/>
                </Border>
                <Border Grid.Row="0" Grid.Column="4" BorderBrush="Black" Background="LightGray" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock Text="測定方法" Margin="2"/>
                </Border>
                <Border Grid.Row="0" Grid.Column="5" BorderBrush="Black" BorderThickness="0.5,1,1,0.5">
                    <TextBlock x:Name="textASSAY_STYLE" Margin="2"/>
                </Border>

                <!-- Data Row 1 -->
                <Border Grid.Row="1" Grid.Column="0" BorderBrush="Black" Background="LightGray" BorderThickness="1,0.5,0.5,0.5">
                    <TextBlock Text="単位" Margin="2"/>
                </Border>
                <Border Grid.Row="1" Grid.Column="1" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="textASSAY_UNIT_NAME" Margin="2"/>
                </Border>
                <Border Grid.Row="1" Grid.Column="2" BorderBrush="Black" Background="LightGray" BorderThickness="0.5">
                    <TextBlock Text="管理資料" Margin="2"/>
                </Border>
                <Border Grid.Row="1" Grid.Column="3" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="textMNG_NAME" Margin="2"/>
                </Border>
                <Border Grid.Row="1" Grid.Column="4" BorderBrush="Black" Background="LightGray" BorderThickness="0.5">
                    <TextBlock Text="Lot No." Margin="2"/>
                </Border>
                <Border Grid.Row="1" Grid.Column="5" BorderBrush="Black" BorderThickness="0.5,0.5,1,0.5">
                    <TextBlock x:Name="textQCLOT_NO" Margin="2"/>
                </Border>

                <!-- Data Row 2 -->
                <Border Grid.Row="2" Grid.Column="0" BorderBrush="Black" Background="LightGray" BorderThickness="1,0.5,0.5,1">
                    <TextBlock Text="N" Margin="2"/>
                </Border>
                <Border Grid.Row="2" Grid.Column="1" BorderBrush="Black" BorderThickness="0.5,0.5,0.5,1">
                    <TextBlock x:Name="textCTRLPARM_DISP_SU" Margin="2"/>
                </Border>
                <Border Grid.Row="2" Grid.Column="2" BorderBrush="Black" Background="LightGray" BorderThickness="0.5,0.5,0.5,1">
                    <TextBlock Text="実施日" Margin="2"/>
                </Border>
                <Border Grid.Row="2" Grid.Column="3"  Grid.ColumnSpan="3" BorderBrush="Black" BorderThickness="0.5,0.5,1,1">
                    <TextBlock x:Name="textJissiDate" Grid.Row="2" Grid.Column="3" Grid.ColumnSpan="3" Margin="2"/>
                </Border>
            </Grid>

            <!-- ■ラベル（Xバー） -->
            <StackPanel Grid.Row="2" Orientation="Horizontal" Margin="1,2,1,2">
                <StackPanel.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="RoyalBlue" Offset="0"/>
                        <GradientStop Color="WhiteSmoke" Offset="0.493"/>
                    </LinearGradientBrush>
                </StackPanel.Background>
                <Label Content="X Bar" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10,0,10,0" />
            </StackPanel>
            <!-- ■チャート（Xバー） -->
            <Grid Grid.Row="3" Margin="1,1,1,1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="80" />
                </Grid.ColumnDefinitions>
                <Border BorderBrush="Black" BorderThickness="1" Margin="5,1,5,1">
                    <lvc:CartesianChart Name="XBarChart" Grid.Column="0">
                    </lvc:CartesianChart>
                </Border>
                <!-- 平均値表示用のラベル -->
                <Grid x:Name="XBarGrid" Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Label x:Name="lblUCLValue" Content="UCL" Grid.Row="0" VerticalAlignment="Top" FontSize="10"/>
                    <Label x:Name="lbl2SDValue" Content="2σ" Grid.Row="1" VerticalAlignment="Top" FontSize="10"/>
                    <Label x:Name="lblAverageValue" Content="CL" Grid.Row="2" VerticalAlignment="Center" FontSize="10"/>
                    <Label x:Name="lblMinus2SDValue" Content="-2σ" Grid.Row="3" VerticalAlignment="Bottom" FontSize="10"/>
                    <Label x:Name="lblLCLValue" Content="LCL" Grid.Row="4" VerticalAlignment="Bottom" FontSize="10"/>
                </Grid>
            </Grid>

            <!-- ■Rs チャートのラベル -->
            <StackPanel Grid.Row="4" Orientation="Horizontal" Margin="1,2,1,2">
                <StackPanel.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="RoyalBlue" Offset="0"/>
                        <GradientStop Color="WhiteSmoke" Offset="0.493"/>
                    </LinearGradientBrush>
                </StackPanel.Background>
                <Label Content="Rs" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10,0,10,0" FontWeight="Bold"/>
            </StackPanel>

            <!-- Rs チャート -->
            <Grid Grid.Row="5"  Margin="1,1,1,1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="80" />
                </Grid.ColumnDefinitions>
                <Border  BorderBrush="Black" BorderThickness="1" Margin="5,1,5,1">
                    <lvc:CartesianChart Name="RsChart" Grid.Column="0">
                    </lvc:CartesianChart>
                </Border>
                <Grid x:Name="RsGrid" Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Label x:Name="lblRsUCL" Content="UCL" Grid.Row="0" VerticalAlignment="Top" FontSize="10"/>
                    <Label x:Name="lblRs2Sigma" Content="2σ" Grid.Row="1" VerticalAlignment="Top" FontSize="10"/>
                    <Label x:Name="lblRsAverageValue"  Content="CL" Grid.Row="2" VerticalAlignment="Center" FontSize="10"/>
                    <Label Content="" Grid.Row="3" VerticalAlignment="Center" FontSize="10"/>
                    <Label x:Name="lblRsLCL" Content="LCL" Grid.Row="4" VerticalAlignment="Bottom" FontSize="10"/>
                </Grid>
            </Grid>

            <!-- ■R チャートのラベル -->
            <StackPanel Grid.Row="6" Orientation="Horizontal" Margin="10,2,2,2">
                <StackPanel.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="RoyalBlue" Offset="0"/>
                        <GradientStop Color="WhiteSmoke" Offset="0.493"/>
                    </LinearGradientBrush>
                </StackPanel.Background>
                <Label Content="R" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10,0,10,0" FontWeight="Bold"/>
            </StackPanel>
            <!-- ■R チャート -->
            <Grid Grid.Row="7" Margin="1,1,1,1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="80" />
                </Grid.ColumnDefinitions>
                <Border BorderBrush="Black" BorderThickness="1" Margin="5,1,5,1">
                    <lvc:CartesianChart Name="RChart" Grid.Column="0">
                    </lvc:CartesianChart>
                </Border>
                <Grid x:Name="RGrid" Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Label x:Name="lblRUCL" Content="UCL" Grid.Row="0" VerticalAlignment="Top" FontSize="10"/>
                    <Label Content="" Grid.Row="1" VerticalAlignment="Top"/>
                    <Label x:Name="lblRAverageValue" Content="CL" Grid.Row="2" VerticalAlignment="Center" FontSize="10"/>
                    <Label Content="" Grid.Row="3" VerticalAlignment="Top" FontSize="10"/>
                    <Label x:Name="lblRLCL" Content="LCL" Grid.Row="4" VerticalAlignment="Bottom" FontSize="10"/>
                </Grid>
            </Grid>

            <!-- データグリッド -->
            <!--<Grid Grid.Row="4" ListBox x:Name="L1"/>-->
            <Grid Name="MainGrid" Grid.Row="8">
                <!-- 列定義 -->
                <Grid.ColumnDefinitions>
                    <!-- 16列を定義 -->
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="auto" />
                </Grid.ColumnDefinitions>

                <!-- 行定義 -->
                <Grid.RowDefinitions>
                    <!-- 17行を定義 -->
                    <RowDefinition Height="Auto" />
                    <!-- ヘッダ行 -->
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <!-- ヘッダ定義 -->
                <Border Grid.Row="0" Grid.Column="0" BorderBrush="Black" BorderThickness="1,1,0.5,1">
                    <TextBlock Text="No" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="1" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="日付" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="2" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="X Bar" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="3" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="Xmax" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="4" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="Xmin" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="5" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="Rs" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="6" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="R" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="7" BorderBrush="Black" BorderThickness="0.5,1,1,1">
                    <TextBlock Text="備考" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="8" BorderBrush="Black" BorderThickness="1,1,0.5,1">
                    <TextBlock Text="No" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="9" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="日付" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="10" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="X Bar" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="11" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="Xmax" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="12" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="Xmin" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="13" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="Rs" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="14" BorderBrush="Black" BorderThickness="0.5,1,0.5,1">
                    <TextBlock Text="R" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>
                <Border Grid.Row="0" Grid.Column="15" BorderBrush="Black" BorderThickness="0.5,1,1,1">
                    <TextBlock Text="備考" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" />
                </Border>

                <!-- データ定義 -->
                <!-- 1行目 -->
                <Border Grid.Row="1" Grid.Column="0" BorderBrush="Black" BorderThickness="1,1,0.5,0.5">
                    <TextBlock x:Name="Seq1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="1" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Date1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="2" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Xbar1" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="3" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Xmax1" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="4" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Xmin1" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="5" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Rs1" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="6" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="R1" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="7" BorderBrush="Black" BorderThickness="0.5,1,1,0.5">
                    <TextBlock x:Name="Lot1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="8" BorderBrush="Black" BorderThickness="1,1,0.5,0.5">
                    <TextBlock x:Name="Seq17" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="9" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Date17" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="10" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Xbar17" HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="11" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Xmax17" HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="12" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Xmin17" HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="13" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Rs17" HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="14" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="R17" HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="1" Grid.Column="15" BorderBrush="Black" BorderThickness="0.5,1,1,0.5">
                    <TextBlock x:Name="Lot17" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <!-- 2行目 -->
                <Border Grid.Row="2" Grid.Column="0" BorderBrush="Black" BorderThickness="1,0.5,0.5,0.5">
                    <TextBlock x:Name="Seq2" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="1" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="Date2" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="2" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="Xbar2" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="3" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="Xmax2" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="4" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="Xmin2" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="5" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="Rs2" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="6" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="R2" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="7" BorderBrush="Black" BorderThickness="0.5,0.5,1,0.5">
                    <TextBlock x:Name="Lot2" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="8" BorderBrush="Black" BorderThickness="1,1,0.5,0.5">
                    <TextBlock x:Name="Seq18" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="9" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Date18" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="10" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Xbar18" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="11" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Xmax18" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="12" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Xmin18" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="13" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="Rs18" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="14" BorderBrush="Black" BorderThickness="0.5,1,0.5,0.5">
                    <TextBlock x:Name="R18" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="2" Grid.Column="15" BorderBrush="Black" BorderThickness="0.5,1,1,0.5">
                    <TextBlock x:Name="Lot18" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <!-- 3行目 -->
                <Border Grid.Row="3" Grid.Column="0" BorderBrush="Black" BorderThickness="1,0.5,0.5,0.5">
                    <TextBlock x:Name="Seq3" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="3" Grid.Column="1" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="Date3" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="3" Grid.Column="2" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="Xbar3" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="3" Grid.Column="3" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="Xmax3" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="3" Grid.Column="4" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="Xmin3" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
                </Border>
                <Border Grid.Row="3" Grid.Column="5" BorderBrush="Black" BorderThickness="0.5">
                    <TextBlock x:Name="Rs3" HorizontalAlignment="Right" Margin="0,0,2,0" VerticalAlignment="Center" FontSize="10" />
