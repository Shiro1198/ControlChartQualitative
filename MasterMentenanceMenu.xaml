<Window x:Class="ControlChart.MasterMentenanceMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlChart"
        mc:Ignorable="d"
        Title="MasterMentenanceMenu" Height="400" Width="400">
    <Grid x:Name="MasterMentenanceMenu1">
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Name="btnControlChart" Content="コントロールチャート" Width="200" Height="50" Margin="0,0,0,10" Click="btnControlChart_Click"/>
            <Button Name="btnTube" Content="チューブ" Width="200" Height="50" Margin="0,0,0,10" Click="btnTube_Click"/>
            <Button Name="btnManagementFactors" Content="管理係数" Width="200" Height="50" Margin="0,0,0,10" Click="btnManagementFactors_Click"/>
            <Button Name="btnExit" Content="終了" Width="200" Height="50" Click="btnExit_Click"/>
        </StackPanel>
    </Grid>
</Window>
